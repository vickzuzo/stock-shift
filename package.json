    "expo-constants": "~14.2.1",
    "expo-font": "~11.1.1",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.4",
    "firebase": "^9.21.0",
    "formik": "^2.2.9",
    "jest": "^29.2.1",
    "jest-expo": "^48.0.2",
